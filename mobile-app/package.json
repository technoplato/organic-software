{
  "name": "mobile-app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "web": "expo start --web",
    "expose": "expo start --tunnel",
    "start:expose": "npx expo start --tunnel --reset-cache",
    "prebuild": "expo prebuild --platform ios",
    "build:ios": "eas build --platform ios --profile development",
    "build:ios:simulator": "eas build --platform ios --profile development-simulator",
    "build:ios:local": "expo run:ios --configuration Debug",
    "build:ios:local:release": "expo run:ios --configuration Release",
    "ios": "expo run:ios --no-bundler",
    "ios:device": "expo run:ios --no-bundler --device"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^1.0.13",
    "@instantdb/react": "^0.20.28",
    "@instantdb/react-native": "^0.20.28",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "11.4.1",
    "ai": "^5.0.28",
    "expo": "53.0.22",
    "expo-application": "~6.1.1",
    "expo-constants": "^17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-linking": "^7.1.7",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.5",
    "expo-speech-recognition": "^2.1.1",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-get-random-values": "~1.11.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "^0.20.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3"
  },
  "private": true
}
